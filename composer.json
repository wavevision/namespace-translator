{
  "name": "wavevision/namespace-translator",
  "description": "Manage Nette app translations on per namespace basis.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "contributte/console": "^0.9",
    "contributte/translation": "^0.8",
    "google/apiclient": "^2.7",
    "nette/application": "^3.1",
    "nette/di": "^3.0",
    "nette/neon": "^3.0",
    "nikic/php-parser": "^4.9",
    "wavevision/di-service-annotation": "^4.0",
    "wavevision/utils": "^2.1"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "php-coveralls/php-coveralls": "^2.2",
    "wavevision/coding-standard": "^6.0",
    "wavevision/nette-tests": "3.1.3"
  },
  "extra": {
    "phpstan": {}
  }
}
